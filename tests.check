#include "between_two_sets.h"
#define SIZE 5
int a[SIZE] = {2, 4, 8, 16, 64};
int b[SIZE] = {32, 64, 128, 256, 512};

#test findGCD_in_A
	ck_assert_int_eq(findGCD_inA(a, SIZE), 64);

#test allOfAIsFactor
	ck_assert_int_eq(findGCD_inA(a, SIZE), 64);	

#test findLCD_in_B
	ck_assert_int_eq(findLCD_inB(b, SIZE), 32);

#test is_factor_for_allOf_B
	ck_assert_int_eq(findLCD_inB(b, SIZE), 32);